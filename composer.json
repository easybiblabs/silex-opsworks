{
    "name": "easybib/opsworks-provider",
    "type": "library",
    "description": "Wraps the opsworks api in somewhat more useful calls",
    "keywords": [
        "aws",
        "opsworks",
        "silex"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Florian Holzhauer",
            "email": "fh-opsworks@fholzhauer.de",
            "homepage": "http://imagineeasy.com/"
        }
    ],
    "require": {
        "aws/aws-sdk-php-silex": "1.*",
        "aws/aws-sdk-php": "2.*",
        "doctrine/cache": "*"
    },
    "require-dev": {
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "EasyBib\\Service\\Opsworks": "src"
        }
    }
}